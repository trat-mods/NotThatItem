{
  "schemaVersion": 1,
  "id": "nti",
  "version": "${version}",
  "name": "Not that item",
  "description": "Stop picking up trash when you don't want to. Introduces a filter that can prevent players from picking up unwanted items.",
  "authors": [
    "trat"
  ],
  "contact": {
    "homepage": "https://github.com/trat-mods/NotThatItem",
    "sources": "https://github.com/trat-mods/NotThatItem",
    "issues": "https://github.com/trat-mods/NotThatItem/issues"
  },
  "license": "GPL-2.0",
  "icon": "assets/nti/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "net.nti.loaders.NtiLoader"
    ],
    "client": [
      "net.nti.loaders.NtiClientLoader"
    ]
  },
  "mixins": [
    "nti.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.15.10",
    "minecraft": "~1.21",
    "java": ">=21",
    "fabric-api": "*"
  }
}